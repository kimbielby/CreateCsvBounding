   "source": [
    "import pyproj\n",
    "\n",
    "crs_british = pyproj.Proj(init='EPSG:27700')\n",
    "crs_wgs84 = pyproj.Proj(init='EPSG:4326')\n",
    "\n",
    "long, lat = pyproj.transform(crs_british, crs_wgs84, 517393.6563, 194035.5469)\n",
    "print(lat, long)"
   ]
